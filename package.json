{
  "name": "@amaui/mongo",
  "version": "1.1.11111",
  "description": "Utils for easier using of mongodb library.",
  "repository": "https://github.com/amaui-org/amaui-mongo.git",
  "author": "Lazar <lazareric2@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "./src/index.ts",
  "keywords": [
    "mongo",
    "mongodb",
    "backend",
    "back-end",
    "web",
    "javascript",
    "js",
    "typescript",
    "express",
    "expressjs",
    "nodejs",
    "node",
    "library",
    "amaui",
    "utils"
  ],
  "scripts": {
    "prebuild": "rimraf build",
    "build": "node utils/js/build.js",
    "release": "yarn test && yarn build && npm publish ./build",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "type": "tsc --noEmit -p tsconfig.json",
    "cov": "cross-env NODE_ENV=test amaui-test",
    "test": "yarn lint && yarn type && yarn build && yarn cov",
    "docker": "docker-compose -p 'amaui' up -d",
    "mongo": "docker compose -p 'amaui' exec mongo1 mongo localhost:27017 /utils/js/mongo/replica-set.js && docker compose -p 'amaui' exec mongo1-test mongo localhost:47017 /utils/js/mongo/replica-set.test.js",
    "postdocker": "yarn mongo"
  },
  "peerDependencies": {
    "mongodb": "^4.2.2"
  },
  "dependencies": {
    "@amaui/date": "^1.0.11434",
    "@amaui/errors": "^1.0.111",
    "@amaui/models": "^1.1.11111",
    "@amaui/log": "^1.0.11145",
    "@amaui/subscription": "^1.0.11151",
    "@amaui/utils": "^1.0.11447",
    "@babel/runtime": "^7.18.3"
  },
  "devDependencies": {
    "@amaui/models": "^1.1.11111",
    "@amaui/test": "^1.0.11111",
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/express": "^4.17.13",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.11.10",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "mongodb": "^4.3.1",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=12.0.0",
    "yarn": ">=1.22"
  }
}